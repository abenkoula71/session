{
	"name": "framework/session",
	"type": "library",
	"description": "The Framework Session Library",
	"keywords": [
		"session"
	],
	"homepage": "https://the-framework.gitlab.io/libraries/session.html",
	"license": "MIT",
	"authors": [
		{
			"name": "Natan Felles",
			"email": "natanfelles@gmail.com"
		}
	],
	"require": {
		"php": ">=7.3",
		"framework/cache": "^1.0",
		"framework/database": "^1.0"
	},
	"require-dev": {
		"ext-xdebug": "*",
		"framework/coding-standard": "^1.0",
		"localheinz/composer-normalize": "^1.3",
		"phpmd/phpmd": "^2.7",
		"phpstan/phpstan": "^0.11",
		"phpunit/phpunit": "^8.3"
	},
	"autoload": {
		"psr-4": {
			"Framework\\Session\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Session\\": "tests/"
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://the-framework.gitlab.io/satis/"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"post-install-cmd": [
			".hooks/init"
		],
		"post-update-cmd": [
			"composer dump-autoload"
		]
	},
	"support": {
		"email": "natanfelles@gmail.com",
		"issues": "https://gitlab.com/the-framework/libraries/session/issues",
		"wiki": "https://gitlab.com/the-framework/libraries/session/wikis/home",
		"source": "https://gitlab.com/the-framework/libraries/session",
		"docs": "https://the-framework.gitlab.io/libraries/session/docs/"
	}
}
