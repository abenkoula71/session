{
	"name": "framework/session",
	"type": "library",
	"description": "The Session package",
	"keywords": [
		"session"
	],
	"homepage": "https://gitlab.com/the-framework/session",
	"license": "proprietary",
	"authors": [
		{
			"name": "Natan Felles",
			"email": "natanfelles@gmail.com"
		}
	],
	"require": {
		"php": ">=7.3",
		"framework/cache": "dev-master",
		"framework/database": "dev-master"
	},
	"require-dev": {
		"ext-xdebug": "*",
		"apigen/apigen": "dev-master#85290e8",
		"friendsofphp/php-cs-fixer": "^2.14",
		"localheinz/composer-normalize": "^1.1",
		"phpmd/phpmd": "^2.6",
		"phpstan/phpstan": "^0.11",
		"phpunit/phpunit": "^7.5"
	},
	"autoload": {
		"psr-4": {
			"Framework\\Session\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Session\\": "tests/"
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://the-framework.gitlab.io/satis/"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"post-install-cmd": [
			".hooks/init"
		],
		"post-update-cmd": [
			"composer dump-autoload"
		]
	},
	"support": {
		"email": "natanfelles@gmail.com",
		"issues": "https://gitlab.com/the-framework/session/issues",
		"wiki": "https://gitlab.com/the-framework/packages/session",
		"source": "https://gitlab.com/the-framework/session",
		"docs": "https://the-framework.gitlab.io/packages/session/api"
	}
}
