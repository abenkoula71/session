{
	"name": "natanfelles/session",
	"type": "library",
	"description": "The Framework Session Library",
	"keywords": [
		"session"
	],
	"homepage": "https://the-framework.gitlab.io/libraries/session.html",
	"license": "MIT",
	"authors": [
		{
			"name": "Natan Felles",
			"email": "natanfelles@gmail.com"
		}
	],
	"require": {
		"php": ">=8.0",
		"ext-memcached": "*",
		"ext-redis": "*",
		"natanfelles/database": "^2.4",
		"natanfelles/log": "^2.0"
	},
	"require-dev": {
		"ext-xdebug": "*",
		"ergebnis/composer-normalize": "^2.14",
		"jetbrains/phpstorm-attributes": "^1.0",
		"natanfelles/coding-standard": "^1.5",
		"phpmd/phpmd": "^2.10",
		"phpstan/phpstan": "^0.12",
		"phpunit/phpunit": "^9.5"
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"Framework\\Session\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Session\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"support": {
		"email": "natanfelles@gmail.com",
		"issues": "https://gitlab.com/the-framework/libraries/session/issues",
		"wiki": "https://gitlab.com/the-framework/libraries/session/wikis/home",
		"source": "https://gitlab.com/the-framework/libraries/session",
		"docs": "https://the-framework.gitlab.io/libraries/session/docs/"
	}
}
